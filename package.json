{
  "name": "image-merge-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "clean": "rimraf ./dist",
    "start": "ng serve",
    "build": "ng build --project=image-merge-frontend",
    "build:prod": "ng build --project=image-merge-frontend --prod",
    "test:watch": "ng test --project=image-merge-frontend",
    "test:headless": "ng test --project=image-merge-frontend --watch=false --browsers=ChromeHeadlessCustom",
    "lint": "ng lint --project=image-merge-frontend",
    "e2e": "ng e2e",
    "build:ci": "npm run lint && npm run clean && npm run test:headless && npm run build:prod"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~10.1.2",
